{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "include": [
    "./*.ts",
    "./*.tsx",
    "./config/**/*.ts",
    "./config/**/*.tsx",
    "./src/**/*.ts",
    "./src/**/*.tsx"
, "src/features/AuthByUsername/model/selectors/getLoginError"  ]
}
